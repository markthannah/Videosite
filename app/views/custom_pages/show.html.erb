<div class="container">

    <!-- Page Heading -->
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header"><%= @custom_page.greeting %>
            </br>
                <small style="white-space: pre-wrap;"><%= @custom_page.message %></small>
            </h1>
        </div>
    </div>
    <hr>
    <!-- /.row -->

    <!-- Videos -->
  <% @custom_page.videos.each do |video| %>
  <div class="row">
      <div class="col-md-7">
        <div class="embed-responsive embed-responsive-16by9">
          <iframe class="embed-responsive-item" src=<%= video.videourl %>></iframe>
        </div>
      </div>
        <div class="col-md-5">
            <h3><%= video.title %></br>
            <small><%= video.description%></small>
            </h3>
        </div>
    </div>
    <hr>
  <% end %>
